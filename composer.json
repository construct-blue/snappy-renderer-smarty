{
  "name": "construct-blue/snappy-renderer-smarty",
  "description": "Rendering strategy with the smarty/smarty template engine for construct-blue/snappy-renderer.",
  "homepage": "https://github.com/construct-blue/snappy-renderer-smarty",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Kleinschuster",
      "email": "snappy@kleinschuster.de",
      "homepage": "https://www.robs.social",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "construct-blue/snappy-renderer": "self.version",
    "smarty/smarty": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Blue\\Snappy\\Renderer\\Strategy\\Smarty\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueTest\\Snappy\\Renderer\\Strategy\\Smarty\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2",
    "phpstan/phpstan": "^1.10.21"
  },
  "scripts": {
    "phpunit": "phpunit",
    "phpstan": "phpstan analyse"
  }
}
